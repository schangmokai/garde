<div class="modal fade" id="myModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
<div class="modal-dialog">
  <div class="modal-content">
    <div class="modal-header">
      <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
      <h4 class="modal-title" id="myModalLabel">Cr√©er un vehicule</h4>
    </div>
    <div class="modal-body">
         <form  action="/vehicules/ajouter/" method="post" role="form" enctype="multipart/form-data">
             
            <div class="row">
              <div class="col-md-6 mb-3">
                <label for="validationDefault01">carte grice expire</label>
                <input type="text" class="form-control" name="carte_grice_expire"  required>
              </div>
              <div class="col-md-6 mb-3">
                <label for="validationDefault02">carte grice issue</label>
                <input type="text" class="form-control" name="carte_grice_issue"  required>
              </div>
            </div>
            <br>
            <div class="row">
              <div class="col-md-6 mb-3">
                <label for="validationDefault03">carte grice num</label>
                <input type="text" class="form-control" name="carte_grice_num"  required>
              </div>
             
              <div class="col-md-6 mb-3">
                <label for="validationDefault06">carte grise place</label>
                <input type="text" class="form-control" name="carte_grise_place"  required>
              </div>
            </div>
            <br>
            <div class="row">
              <div class="col-md-6 mb-3">
                <label for="validationDefault05">code</label>
                <input type="text" class="form-control" name="code"  required>
              </div>
              <div class="col-md-6 mb-3">
                <label for="validationDefault06">code gps</label>
                <input type="text" class="form-control" name="code_gps" required>
              </div>
            </div>
            <br>
            <div class="row">
             
              <div class="col-md-6 mb-3">
                <label for="validationDefault08">color</label>
                <input type="text" class="form-control" name="color" required>
              </div>

               <div class="col-md-6 mb-3">
                <label for="validationDefault04">date_fabri</label>
                <input type="text" class="form-control" name="date_fabri" required>
              </div>
            </div>
            <br>
            <div class="row">
             
              <div class="col-md-6 mb-3">
                <label for="validationDefault08">description</label>
                <input type="text" class="form-control" name="description"  required>
              </div>

               <div class="col-md-6 mb-3">
                <label for="validationDefault04">imatriculation</label>
                <input type="text" class="form-control" name="imatriculation" required>
              </div>
            </div>
            <br>
            <div class="row">
             
              <div class="col-md-6 mb-3">
                <label for="validationDefault08">latitude</label>
                <input type="text" class="form-control" name="lat" required>
              </div>

               <div class="col-md-6 mb-3">
                <label for="validationDefault04">longitude</label>
                <input type="text" class="form-control" name="lon" required>
              </div>
            </div>
            <div class="row">
             
              <div class="col-md-6 mb-3">
                <label for="validationDefault08">photos_1</label>
                <input type="text" class="form-control" name="photos_1" required>
              </div>

               <div class="col-md-6 mb-3">
                <label for="validationDefault04">photos_2</label>
                <input type="text" class="form-control" name="photos_2" required>
              </div>
            </div>
            <div class="row">
             
              <div class="col-md-6 mb-3">
                <label for="validationDefault08">photos_3</label>
                <input type="text" class="form-control" name="photos_3" required>
              </div>

               <div class="col-md-6 mb-3">
                <label for="validationDefault04">photos_4</label>
                <input type="text" class="form-control" name="photos_4" required>
              </div>
            </div>
            <div class="row">
             
              <div class="col-md-6 mb-3">
                <label for="validationDefault08">photos_5</label>
                <input type="text" class="form-control" name="photos_5" required>
              </div>

               
            </div>
            <br>
            <div class="row">
              <div class="col-md-6 mb-3">
                <label for="validationDefault04">num_chaisis</label>
                <input type="text" class="form-control" name="num_chaisis" placeholder="email de l'utilisateur" required>
              </div> 
              <div class="col-md-6 mb-3">
                 <label for="validationDefault07">Utilisateurs</label>
                 <select class="form-control" name="utilisateurId" >     
                    <% listutilisateurs.forEach(function(todo, index) { %>
                            <option value="<%= todo.id %>"><%= todo.nom %></option>
                    <% }); %>
                </select>
              </div>
            </div>

            <div class="row">
              <div class="col-md-6 mb-3">
                <label for="validationDefault07">Type Vehicule </label>
                 <select class="form-control" name="typesVehiculeId" >     
                    <% listtypes_vehicules.forEach(function(todo, index) { %>
                            <option value="<%= todo.id %>"><%= todo.name %></option>
                    <% }); %>
                </select>
              </div> 
              <div class="col-md-6 mb-3">
                 <label for="validationDefault07">Serie Vehicule</label>
                 <select class="form-control" name="serieVehiculeId" >     
                    <% listserie_vehicule.forEach(function(todo, index) { %>
                            <option value="<%= todo.id %>"><%= todo.name %></option>
                    <% }); %>
                </select>
              </div>
            </div>
            <br>

            <div class="modal-footer">
              <div class="row">
                <div class="col-md-6">
                  <button type="button" class="btn btn-danger btn-lg width-100" data-dismiss="modal"><span class="glyphicon glyphicon-remove"></span> Annuler</button>
                </div>
                <div class="col-md-6">
                  <button class="btn btn-primary btn-lg width-100" type="submit"><span class="glyphicon glyphicon-ok-sign"></span> Enregistrer</button>
                </div>
              </div>
            </div>
          </form>
    </div>
  </div>
</div>
</div>
